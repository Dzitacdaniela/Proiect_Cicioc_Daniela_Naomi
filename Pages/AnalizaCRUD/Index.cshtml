@page
@model Proiect_Cicioc_Daniela_Naomi.Pages.AnalizaCRUD.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Analiza[0].Denumire_Analiza)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Analiza[0].Pret_Analiza)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Analiza[0].Laboranti)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Analiza) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Denumire_Analiza)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pret_Analiza)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Laboranti.Nume_Laborant)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
